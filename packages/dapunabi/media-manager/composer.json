{
  "name": "dapunabi/media-manager",
  "description": "Multi-tenant media manager for Laravel: uploads, variants, shares, quotas.",
  "type": "library",
  "license": "proprietary",
  "version": "0.1.0",
  "require": {
    "php": ">=8.2",
    "illuminate/support": ">=10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Dapunabi\\Media\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dapunabi\\Media\\MediaServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}

